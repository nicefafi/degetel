{
    "name": "degetel/drupal-cms",
    "type": "project",
    "description": "DEGETEL â€” CMS Drupal 9.",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Farouk DHAHRI",
            "role": "Tech Drupal"
        }
    ],
    "require": {
        "composer/installers": "^1.9",
        "cweagans/composer-patches": "^1.7",
        "drupal/admin_toolbar": "^3.0",
        "drupal/bfd": "^2.65",
        "drupal/bricks": "2.x-dev@dev",
        "drupal/color_field": "^2.4",
        "drupal/config_split": "^1.7",
        "drupal/console": "^1.9",
        "drupal/core": "^9.2",
        "drupal/core-composer-scaffold": "^9.2",
        "drupal/devel": "^4.1",
        "drupal/devel_kint_extras": "^1.0",
        "drupal/eck": "1.x-dev@dev",
        "drupal/entity_browser": "^2.6",
        "drupal/field_group": "^3.1",
        "drupal/gin": "^3.0@alpha",
        "drupal/inline_entity_form": "^1.0@RC",
        "drupal/media_bulk_upload": "^1.0@alpha",
        "drupal/menu_link_attributes": "^1.2",
        "drupal/metatag": "^1.16",
        "drupal/module_filter": "^3.2",
        "drupal/multiselect": "^2.0@alpha",
        "drupal/redirect": "^1.6",
        "drupal/robotstxt": "^1.4",
        "drupal/taxonomy_manager": "^2.0",
        "drupal/translatable_menu_link_uri": "^2.0",
        "drupal/webform": "^6.0",
        "drupal/yearonly": "^9.0",
        "drush/drush": "^10",
        "ergebnis/composer-normalize": "^2.6",
        "firephp/firephp-core": "^0.5.3",
        "jakub-onderka/php-parallel-lint": "^1.0",
        "kint-php/kint": "^3.3",
        "phing/phing": "^2.16.4",
        "vlucas/phpdotenv": "^5.1"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "config": {
        "autoloader-suffix": "Drupal9",
        "discard-changes": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "vendor-dir": "vendor"
    },
    "extra": {
        "drupal-scaffold": {
            "locations": {
                "web-root": "./html"
            }
        },
        "enable-patching": true,
        "installer-paths": {
            "html/core": [
                "type:drupal-core"
            ],
            "html/libraries/{$name}": [
                "type:drupal-library"
            ],
            "html/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "html/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "html/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "html/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "html/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ],
            "drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ]
        },
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "patches": {
        
        }
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ]
    },
    "repositories": {
        "drupal": {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    }
}
